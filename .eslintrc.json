{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "xo",
        "plugin:react/recommended"
    ],
    "overrides": [
        {
            "extends": [
                "xo-typescript"
            ],
            "files": [
                "*.ts",
                "*.tsx"
            ]
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react", 
        "react-hooks", 
        "@typescript-eslint", 
        "prettier"
    ],
    "rules": {
    }
}
